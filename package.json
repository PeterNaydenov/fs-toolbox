{
  "name": "fs-toolbox",
  "version": "3.1.10",
  "description": "File System Toolbox",
  "main": "src/fs-toolbox.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/fs-toolbox.js",
      "require": "./dist/fsbox.cjs",
      "default": "./src/fs-toolbox.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./src/*": "./src/*"
  },
  "dependencies": {
    "ask-for-promise": "3.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "c8": "^10.1.3",
    "chai": "6.2.0",
    "mocha": "11.7.4",
    "rollup": "^4.52.5"
  },
  "scripts": {
    "test": "mocha test",
    "cover": "c8 mocha test",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeterNaydenov/fs-toolbox.git"
  },
  "keywords": [
    "fs",
    "file",
    "folder",
    "toolbox"
  ],
  "author": "Peter Naydenov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PeterNaydenov/fs-toolbox/issues"
  },
  "homepage": "https://github.com/PeterNaydenov/fs-toolbox#readme",
  "c8": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "node_modules",
      "test",
      "test-data"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
